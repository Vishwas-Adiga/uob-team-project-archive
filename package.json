    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.8.1",
    "sass": "^1.58.3",
    "vite-express": "^0.3.1"
  },
  "devDependencies": {
    "@types/carbon__icons-react": "^11.15.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
